<xml id="toolboxFunction" style="display: none">

  <sep gap="8"></sep>



  <category name="Configuration" id="config-category" colour="180">
    <!-- It's gonna be filled with config variables -->
  </category>

  <sep gap="8"></sep>

  <category name="Fonctions" custom="PROCEDURE" colour="80"></category>
  <category name="Variables" custom="VARIABLE" colour="100"></category>

  <category name="Briques de base" colour="250">
    <block type="decodebytes"></block>
    <block type="decodeunsignedinteger"></block>
    <block type="decodesignedinteger"></block>
    <block type="compute"></block>
    <block type="execute"></block>
    <block type="switch"></block>
    <block type="case"></block>
    <block type="default"></block>
